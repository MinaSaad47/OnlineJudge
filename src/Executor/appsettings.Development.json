{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "Masstransit": {
    "RabbitMQ": {
      "Host": "localhost"
    }
  },
  "Isolate": {
    "MemoryLimitInKB": 512000,
    "StackLimitInKB": 512000,
    "TimeLimitInSec": 10,
    "WallTimeLimitInSec": 0,
    "ExtraTimeLimitInSec": 0,
    "ProcessCountLimit": 10,
    "EnvironmentVariables": [
      "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
    ]
  }
}
